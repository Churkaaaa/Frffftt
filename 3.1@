var students = ["адам", "рома", "миша", "антон", "влад", "кирилл", "андрей", "никита", "егор", "матвей", "леша", "марк"]
print("Список учеников:")
for student in students {
    print(student)
}

students.append("даша")
students.append("лена")
students.append("рита")
students.insert("зоя", at: 3)
students.insert("люда", at: 6)
students.insert("катя", at: 9)
students.insert("марина", at: 12)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
