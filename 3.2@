var students = ["адам", "рома", "миша", "илья", "никита", "влад", "кирилл", "марк", "ваня", "даниил", "егор", "матвей", "маша", "лена", "катя", "марина", "зоя", "вера", "олеся"]
print("Исходный список учеников:")
for student in students {
    print(student)
}

students.removeSubrange(0...6)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
